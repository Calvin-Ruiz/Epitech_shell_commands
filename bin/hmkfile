#!/bin/bash
cd "include"
echo -e -n "/*\n** EPITECH PROJECT, 2019\n** $1\n** File description:\n** $2.h\n*/\n\n#ifndef " > "$2.h"
echo "$2_H_" | tr [a-z] [A-Z] >> "$2.h"
echo -e -n "#define " >> "$2.h"
echo "$2_H_" | tr [a-z] [A-Z] >> "$2.h"
echo -e -n "\n\n\n#endif /* " >> "$2.h"
echo -e -n "$2_H_" | tr [a-z] [A-Z] >> "$2.h"
echo -e -n " */" >> "$2.h"
